{
  "name": "rawkit",
  "version": "1.0.4",
  "description": "Open chrome devtools debugger",
  "bin": "./index.js",
  "files": [
    "./index.js"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "lint": "eslint .",
    "admin": "node ./scripts/admin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darcyclarke/rawkit.git"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.4.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "mocha": "3.4.2",
    "pre-git": "3.14.0"
  },
  "keywords": [
    "profile",
    "darcy",
    "darcyclarke",
    "developer",
    "designer",
    "strategy",
    "ux",
    "web",
    "development",
    "node",
    "javascript",
    "oss",
    "css",
    "html"
  ],
  "author": "Darcy Clarke <darcy@darcyclarke.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darcyclarke/rawkit/issues"
  },
  "homepage": "https://github.com/darcyclarke/rawkit#readme",
  "config": {
    "commit-msg": "conventional",
    "pre-git": {
      "pre-push": [
        "echo 'Pre-push checks...' && exit 0",
        "npm run lint",
        "npm test"
      ]
    }
  },
  "dependencies": {
    "opn": "5.0.0",
    "yargs": "8.0.1"
  }
